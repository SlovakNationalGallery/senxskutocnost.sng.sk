<h4>{{ title }}</h4>
<div class="uk-grid subobject-overview not-extended">
	{% for subobject in subobjects %}
        <div class="uk-width-1-1 uk-width-small-1-3 subobject not-extended">
        	<a href="{{subobject.href}}">
				<img src="{{ subobject.thumbnail.url }}" class="" alt="subobject">
        	</a>
        	<a href="{{subobject.href}}">
        		<img src="{{ theme_url }}/images/subobj_link.svg" class="icon" alt="">{{ subobject.name }}
        	</a>
			{# <p class="f-uni-grotesk-regular"></p> #}
		</div>
    {% endfor %}
</div>